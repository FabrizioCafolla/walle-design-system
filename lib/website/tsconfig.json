{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist", "node_modules", "public"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@walle/*": ["src/@walle/*"],
      "@utils": ["src/utils/index.js"],
      "@styles/*": ["src/styles/*"],
      "@utils/*": ["src/utils/*"],
      "@content/*": ["src/content/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"]
    },
    "strictNullChecks": true
  }
}
