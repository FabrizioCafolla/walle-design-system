{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist", "node_modules", "public"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@walle/*": ["src/@walle/*"],

      "@components": ["src/@walle/components/index.js"],
      "@components/*": ["src/@walle/*"],

      "@layouts": ["src/@walle/layouts/index.js"],
      "@layouts/*": ["src/@walle/layouts/*"],

      "@config": ["src/@walle/config.ts"],
      "@styles/*": ["src/@walle/styles/*"],

      "@utils": ["src/utils/index.js"]
    },
    "strictNullChecks": true
  }
}
